
{block content}

<div class="wrapperBox">
<h1>Uplatnění pozvánky</h1>

    <div class="box">
    {form invitationApplication}
    <ul class="form-errors" n:if="$form->hasErrors()">
        <li n:foreach="$form->errors as $error" class="form-error">{$error}</li>
    </ul>

        <table>
            <tr><td>{label token /}</td></tr>
            <tr><td>{input token}</td></tr>
            <tr><td>{input send}</td></tr>
        </table>
    {/form}
    </div>
</div>

{/block}